#!/bin/bash
cp -f ./package.json ./tests/fixtures/example-app/package.json &&
sed -i -e 's/"track-builder"/"example-app"/g' ./tests/fixtures/example-app/package.json &&
sed -i -e 's/"scripts": {/"scripts": {\n"build": "webpack -p",/g' ./tests/fixtures/example-app/package.json &&
cd ./tests/fixtures/example-app &&
npm install &&
npm run build
